<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >

    <LinearLayout
        android:id="@+id/tp_ll_ad_choices"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


    </LinearLayout>

    <LinearLayout
        android:id="@+id/tp_ll_nativebanner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:orientation="horizontal">


        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/tp_native_icon_image"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:visibility="visible" />

        </FrameLayout>


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="vertical"
            android:paddingLeft="4dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tp_native_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:lines="1"
                    android:text="hello ooooo"
                    android:textColor="@android:color/black"
                    android:textSize="13sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <TextView
                android:id="@+id/tp_native_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:lines="1"
                android:text=""
                android:textSize="10sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                >

                <ImageView
                    android:layout_width="12dp"
                    android:layout_height="10dp"
                    android:background="@mipmap/tp_ad" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/tp_star_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="gone">

                <RatingBar
                    android:id="@+id/tp_native_star"
                    style="?android:attr/ratingBarStyleSmall"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:isIndicator="true"
                    android:rating="3.5"
                    android:stepSize="0.5" />

                <TextView
                    android:id="@+id/tp_star_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:lines="1"
                    android:text="5.0"
                    android:textSize="12sp" />

            </LinearLayout>
        </LinearLayout>


        <RelativeLayout
            android:id="@+id/tp_native_main_image"
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:layout_below="@+id/tp_native_text"
            android:layout_alignParentLeft="true">

            <ImageView
                android:id="@+id/tp_mopub_native_main_image"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="60dp"
            android:layout_height="50dp"
            >

            <ImageView
                android:id="@+id/tp_native_ad_choice"
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:paddingTop="2dp"
                android:paddingEnd="2dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                />

            <FrameLayout
                android:id="@+id/tp_ad_choices_container"
                android:paddingTop="2dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:visibility="visible"
                />

            <Button
                android:id="@+id/tp_native_cta_btn"
                android:layout_width="60dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:paddingLeft="3dp"
                android:paddingRight="3dp"
                android:text="hello oooooooooo"
                android:background="#4286F4"
                android:textColor="@android:color/white"
                android:textSize="12sp"
                android:visibility="visible" />

        </RelativeLayout>

    </LinearLayout>
</RelativeLayout>
